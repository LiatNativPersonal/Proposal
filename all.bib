@Article{Basow2003,
author="Basow, Susan A. and Rubenfeld, Kimberly",
title="``{T}roubles Talk'': Effects of Gender and Gender-Typing",
journal="Sex Roles",
year="2003",
month="February",
volume="48",
number="3",
pages="183--187",
abstract="Effects of gender and gender-typing on communication styles were examined in 62 male and 110 female college students. Participants were asked to rate the likelihood of giving certain responses to a friend's problems as well as the likelihood of feeling certain emotions when a friend gives them advice or sympathy for their own problems. Men and masculine people were expected to be more likely to respond to a friend in a way that reduces interpersonal intimacy; women and feminine people were expected to respond in a way that enhances interpersonal intimacy. Although a few gender differences were found, participants' level of expressive/nurturing traits (``femininity'') was related to more responses than was gender. The importance of examining gender-linked traits and not just gender when studying communication patterns is emphasized.",
issn="1573-2762",
doi="10.1023/A:1022411623948",
url="https://doi.org/10.1023/A:1022411623948"
}

